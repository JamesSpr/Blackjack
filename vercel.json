{
    "version": 2,
    "builds": [
        {
            "src": "./api/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "./frontend/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}